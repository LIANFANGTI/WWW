var chars = ['0','1','2','3','5','6','7','8','9'];
function generateMixed(n) {
     var res = "";
     for(var i = 0; i < n ; i ++) {
         var id = Math.ceil(Math.random()*8);
         res += chars[id];
     }
     return res;
}
